{
  "_version": "1.58.0",
  "sap.app": {
    "id": "ui5.cod3rsgrowth",
    "i18n": "i18n/i18n.properties",
    "title":"{{tituloApp}}",
    "description": "{{descricaoApp}}",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes":{
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5":{
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ui5.cod3rsgrowth.i18n.i18n",
          "supportedLocales": [
            ""
          ],
          "fallbackLocale": ""
        }
      }
    },
    "rootView":{
      "viewName": "ui5.cod3rsgrowth.view.App",
      "type": "XML",
      "id": "app"
    },
    "resources":{
      "css":[
        {
          "uri":"css/styles.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "ui5.codersgrowth.app",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
        "target": "notFound"
        },
        "async": "true"
      },
      "routes":[{
        "pattern": "",
        "name": "paginaPrincipal",
        "target": "paginaPrincipal"
      }

      ],
      "targets": {
        "inicio":{
          "id": "paginaPrincipal",
          "nome":"PaginaPrincipal",
          "level": 1
        },
        
			"notFound": {
        "path": "ui5.codersgrowth.app.naoEncontrada",
          "id": "naoEncontrada",
          "name": "NaoEncontrada",
          "transition": "show"
          }
        }
      }
    }
  }