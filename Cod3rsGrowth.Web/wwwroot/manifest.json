{
  "_version": "1.58.0",
  "sap.app": {
    "id": "ui5.cod3rsgrowth",
    "i18n": "i18n/i18n.properties",
    "title": "{{tituloApp}}",
    "description": "{{descricaoApp}}",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ui5.cod3rsgrowth.i18n.i18n",
          "supportedLocales": [
            ""
          ],
          "fallbackLocale": ""
        }
      },
      "naturezaJuridica":{
        "type": "sap.ui.model.json.JSONModel",
        "uri": "modelos/ModelosJson/ValoresNaturezaJuridica.json"
      },
      "estados":{
        "type": "sap.ui.model.json.JSONModel",
        "uri": "modelos/ModelosJson/ValoresEstados.json"
      },
      "habilitado":{
        "type": "sap.ui.model.json,JSONModel",
        "uri": "modelos/ModelosJson/ValoresHabilitado.json"
      }
    },
    "rootView": {
      "viewName": "ui5.cod3rsgrowth.view.App",
      "type": "XML",
      "id": "app"
    },
    "resources": {
      "css": [
        {
          "uri": "i18n/css/styles.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "type": "view",
        "viewType": "XML",
        "path": "ui5.cod3rsgrowth.view",
        "controlId": "app",
        "controlAggregation": "detailPages",
        "transition": "slide",
        "bypassed": {
          "target": "naoEncontrada"
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "ConveniosInicio",
          "target": "lista"
        },
        {
          "pattern": "empresas",
          "name": "Empresas",
          "target": "lista"
        },
        {
          "pattern": "escolas",
          "name": "Escolas",
          "target": "lista"
        }
      ],
      "targets": {
        "naoEncontrada": {
          "viewName": "NaoEncontrada",
          "viewType": "XML",
          "viewId": "naoEncontrada"
        },
        "lista": {
          "viewName": "Lista",
          "viewType": "XML",
          "viewId": "lista",
          "viewLevel": 1,
          "controlAggregation": "detailPages"
        }
      }
    }
  }
}